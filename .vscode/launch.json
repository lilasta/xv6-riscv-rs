{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "gdb",
			"request": "attach",
			"name": "GDB",
			"executable": "${workspaceFolder}/xv6/kernel/kernel",
			"target": ":26000",
			"remote": true,
			"cwd": "${workspaceRoot}",
			"valuesFormatting": "parseText",
			"gdbpath": "rust-gdb",
			"env": {
				"RUST_GDB": "riscv64-elf-gdb",
			}
		}
	]
}