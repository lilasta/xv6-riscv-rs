{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "gdb",
			"request": "attach",
			"name": "Attach to gdbserver",
			"executable": "${workspaceFolder}/xv6/kernel/kernel",
			"target": ":26000",
			"remote": true,
			"cwd": "${workspaceRoot}",
			"valuesFormatting": "parseText",
			"gdbpath": "riscv64-elf-gdb",
			"debugger_args": [
				"--init-eval-command=set riscv use-compressed-breakpoints yes"
			]
		}
	]
}